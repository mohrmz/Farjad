<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationIcon>Farjad.ico</ApplicationIcon>
    <AssemblyVersion>2.0.1</AssemblyVersion>
    <FileVersion>2.0.1</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>Farjad.png</PackageIcon>
    <Version>2.0.1</Version>
    <Authors>Mohammad Ramezani</Authors>
    <Company>Farjad</Company>
    <Description>Farjad is a powerful library for Domain Driven Design architecture, CQRS pattern, event driven development</Description>
    <PackageDescription>Farjad is a powerful library for Domain Driven Design architecture, CQRS pattern, event driven development</PackageDescription>
    <PackageProjectUrl>https://github.com/mohrmz/Farjad</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mohrmz/Farjad</RepositoryUrl>
    <PackageReleaseNotes>Publish Github Packages</PackageReleaseNotes>
    <Copyright>All rights reserved for Mohammad Ramezani</Copyright>
    <PackageTags>ddd-architecture,cqrs-pattern,event-driven</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Farjad.ico" />
    <None Include="Farjad.png" Pack="true" PackagePath="" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.123" />
    <PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Caching.SqlServer" Version="6.0.7" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="6.0.1" />
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="6.0.0" />
    <PackageReference Include="Farjad.Extensions.Caching.Abstractions" Version="2.0.1" />
    <PackageReference Include="Farjad.Extensions.Serializers.Abstractions" Version="2.0.1" />
  </ItemGroup>

</Project>