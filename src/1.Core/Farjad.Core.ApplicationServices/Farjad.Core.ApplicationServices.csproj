<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationIcon>Farjad.ico</ApplicationIcon>
    <ApplicationIcon>Farjad.ico</ApplicationIcon>
    <AssemblyVersion>2.0.1</AssemblyVersion>
    <FileVersion>2.0.1</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>Farjad.png</PackageIcon>
    <Version>2.0.1</Version>
    <Authors>Mohammad Ramezani</Authors>
    <Company>Farjad</Company>
    <Description>Farjad is a powerful library for Domain Driven Design architecture, CQRS pattern, event driven development</Description>
    <PackageDescription>Farjad is a powerful library for Domain Driven Design architecture, CQRS pattern, event driven development</PackageDescription>
    <PackageProjectUrl>https://github.com/mohrmz/Farjad</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mohrmz/Farjad</RepositoryUrl>
    <PackageReleaseNotes>Publish Github Packages</PackageReleaseNotes>
    <Copyright>All rights reserved for Mohammad Ramezani</Copyright>
    <PackageTags>ddd-architecture,cqrs-pattern,event-driven</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Farjad.ico" />
    <None Include="Farjad.png" Pack="true" PackagePath="" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FluentValidation" Version="11.1.0" />
    <PackageReference Include="Farjad.Utilities" Version="2.0.1" />
    <PackageReference Include="Farjad.Extensions.Logger.Abstractions" Version="2.0.1" />
    <PackageReference Include="Farjad.Extensions.Translations.Abstractions" Version="2.0.1" />
    <PackageReference Include="Farjad.Core.Contracts" Version="2.0.1" />
    <PackageReference Include="Farjad.Core.Domain" Version="2.0.1" />
  </ItemGroup>

</Project>
